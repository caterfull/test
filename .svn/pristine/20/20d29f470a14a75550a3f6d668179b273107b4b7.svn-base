@font-face {
    font-family: 'RobotoCondensed-Light';
    src: url('../fonts/RobotoCondensed-Light.ttf') format('truetype');
}
.login-logo-img {text-align: center}
.login-logo-img img{
    margin-left: -8px;
    width: 60%;
    margin-bottom: 40px;
}
.fa-envelope.form-control-feedback,.fa-lock.form-control-feedback{
    top:35px !important;
}
.register-box .has-feedback .form-control-feedback{
    top:10px !important;
}
.text-validation-error{
    color: #CC0000;
}

.skin-grey .main-header .logo img{
    max-width: 200px;
}
.skin-grey .main-header .logo .logo-mini img{
    max-width: 42px;
}
.table-user-image{
    margin-right: 0;
}
.crud-actions{
    margin-bottom: 15px;
}
.form-group.no-list ul{
    list-style: none;
    padding-left: 0;
    font-size: 10pt;

}
.form-group.no-list ul label{
    font-weight: 100 !important;
}

.alert{    border-left: 5px solid #eee;}
.alert.alert-danger,
.alert.alert-error{
    border-color: #c23321;
}

.alert.alert-success{  border-color: #00733e;}
.alert.alert-info{  border-color: #0097bc;}
.alert.alert-warning{  border-color: #c87f0a;}

